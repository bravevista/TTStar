{
  "expo": {
    "name": "TTStar",
    "description": "App movil de TrutThreads",
    "slug": "trustthreads",
    "owner": "trustthreads",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/bravevista/TTStar",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "notification": {},
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir a la aplicación acceder a tu galería para que puedas compartirlas con tus amigos.",
          "cameraPermission": "Permitir a la aplicación el acceso a tu cámara.",
          "microphonePermission": "Permite a la aplicación el acceso a tu micrófono."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1fea8c92-d4d4-417e-85ed-0ff684e7f5d9"
      }
    }
  }
}

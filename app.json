{
  "expo": {
    "name": "TTStar",
    "description": "App movil de TrutThreads",
    "slug": "TTStar",
    "owner": "bravevista",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/bravevista/TTStar",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "notification": {},
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Poppins-Regular.ttf"] 
        }
      ]
    ]
  }
}
